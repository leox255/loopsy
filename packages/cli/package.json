{
  "name": "@loopsy/cli",
  "version": "1.0.0",
  "description": "CLI management tool for Loopsy",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "loopsy": "dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@loopsy/protocol": "workspace:*",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "undici": "^7.0.0",
    "yaml": "^2.4.0",
    "yargs": "^17.7.0"
  },
  "devDependencies": {
    "typescript": "^5.5.0",
    "tsx": "^4.16.0",
    "vitest": "^2.0.0",
    "@types/node": "^22.0.0",
    "@types/yargs": "^17.0.0"
  }
}
